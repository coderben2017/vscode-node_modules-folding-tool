{
	"name": "node-modules-folder",
	"displayName": "快速收起node_modules目录",
	"description": "快速收起node_modules目录，免去鼠标点点点的麻烦",
	"version": "0.0.3",
	"keywords": [
		"vscode",
		"plugin",
		"node_modules",
		"fold"
	],
	"icon": "logo.png",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Keymaps"
	],
	"publisher": "CoderBen",
	"main": "./dist/extension.js",
	"activationEvents": [
		"onCommand:node-modules-folder.fold"
	],
	"contributes": {
		"commands": [
			{
				"command": "node-modules-folder.fold",
				"title": "node-modules-folder: fold node_modules"
			}
		],
		"keybindings": [
			{
				"command": "node-modules-folder.fold",
        "key": "alt+w",
        "mac": "alt+w"
      }
		]
	},
	"repository": {
		"url": "https://github.com/coderben2017/vscode-node_modules-folding-tool"
	},
	"scripts": {
		"build": "vsce package",
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.55.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"ts-loader": "^8.0.14",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	}
}
